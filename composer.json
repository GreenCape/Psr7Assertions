{
    "name": "maks3w/psr7-assertions",
    "description": "Test your library conformance with PSR-7",
    "keywords": [
        "assertion",
        "log",
        "phpunit",
        "psr7",
        "psr-7"
    ],
    "license": "BSD-2-Clause",
    "homepage": "https://github.com/Maks3w/Psr7Assertions",
    "require": {
        "php": ">= 5.4",
        "psr/http-message": "~1.0",
        "phpunit/phpunit": "^4 || ^5"
    },
    "require-dev": {
        "fabpot/php-cs-fixer": "1.9.*",
        "maks3w/phpunit-methods-trait": "^4.6"
    },
    "autoload": {
        "psr-4": {
            "Maks3w\\Psr7Assertions\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Maks3w\\Psr7AssertionsTest\\": "test/"
        }
    }
}
