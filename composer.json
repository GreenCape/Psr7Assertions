{
    "name": "maks3w/psr7-assertions",
    "description": "Test your library conformance with PSR-7",
    "keywords": [
        "assertion",
        "log",
        "phpunit",
        "psr7",
        "psr-7"
    ],
    "license": "BSD-2-Clause",
    "authors": [
        {
            "name": "Maks3w",
            "email": "github.maks3w@virtualplanets.net"
        }
    ],
    "homepage": "https://github.com/Maks3w/Psr7Assertions",
    "require": {
        "php": ">= 5.4",
        "psr/http-message": "~1.0",
        "phpunit/phpunit": "~4.0"
    },
    "require-dev": {
        "fabpot/php-cs-fixer": "~1.8"
    },
    "autoload": {
        "psr-4": {
            "Maks3w\\Psr7Assertions\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Maks3w\\Psr7AssertionsTest\\": "test/"
        }
    }
}
